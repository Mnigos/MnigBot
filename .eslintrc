{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "quotes": ["error", "single"],
        "indent": ["error", 4]
    },
    "env": {
        "node": true,
        "es6": true
    }
}